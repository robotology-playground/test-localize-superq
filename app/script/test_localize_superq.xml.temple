application>
    <name>Test Localize Superquadric</name>

    <dependencies>
        <port>/instanceSegmenter/rpc</port>
        <port>/memory/rpc</port>
    </dependencies>

    <module>
        <name>test-localize-superq</name>
        <node>localhost</node>
    </module>

    <module>
        <name>localize-superquadric</name>
        <node>localhost</node>
    </module>

    <module>
        <name>point-cloud-read</name>
        <node>localhost</node>
    </module>

    <connection>
        <from>/test-localize-superquadric/pointCloud:rpc</from>
        <to>/pointCloudRead/rpc</to>
        <protocol>fast_tcp</protocol>
    </connection>

    <connection>
        <from>/point-cloud-read/points:rpc</from>
        <to>/isntanceSegmenter/rpc</to>
        <protocol>fast_tcp</protocol>
    </connection>

    <connection>
        <from>/test-localize-superquadric/localizeSuperq:rpc</from>
        <to>/localize-superquadric/service:rpc</to>
        <protocol>fast_tcp</protocol>
    </connection>

</application>
